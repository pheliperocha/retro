{
  "name": "retrospective-desktop",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check",
    "build": "ng build --prod",
    "test": "ng test",
    "test:no-watch": "ng test --browsers ChromeHeadless --watch false",
    "test:coverage": "ng test --browsers ChromeHeadless --watch false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ci": "npm run lint && npm run test:no-watch && npm run e2e && ng build --prod",
    "postinstall": "ng build --prod",
    "release": "standard-version"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/cdk": "^7.2.0",
