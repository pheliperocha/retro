    "@angular/flex-layout": "^7.0.0-beta.22",
    "@angular/forms": "^7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/platform-server": "^7.1.4",
    "@angular/router": "^7.1.4",
    "body-parser": "^1.18.3",
    "core-js": "^2.5.7",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "jwt-decode": "^2.2.0",
    "ng-socket-io": "^0.2.4",
    "ng2-dragula": "^1.5.0",
    "ngx-swiper-wrapper": "^10.0.0",
    "node-sass": "^4.11.0",
